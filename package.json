{
  "name": "shadowsocks-gui",
  "main": "index.html",
  "description": "shadowsocks client with GUI",
  "version": "0.4.1",
  "keywords": [
    "shadowsocks",
    "node-webkit"
  ],
  "single-instance": false,
  "dependencies": {
    "args": "0.0.3",
    "flour": "~0.5.8",
    "hydrate": "git://github.com/nanodeath/HydrateJS",
    "jade": "~1.3.1",
    "sh": "0.0.3",
    "shadowsocks": "1.5.2",
    "uglify-js": "~2.4.15"
  },
  "window": {
    "icon": "icon.png",
    "toolbar": false,
    "width": 500,
    "height": 250,
    "position": "mouse",
    "resizable": true
  },
  "js-flags": "--expose-gc",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "devDependencies": {
    "chai": "^1.10.0",
    "coffeelint": "^1.8.1",
    "grunt": "^0.4.5",
    "grunt-coffeelint": "0.0.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-coffee": "^0.12.0",
    "grunt-contrib-jade": "^0.14.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-docco": "^0.3.3",
    "grunt-newer": "^1.1.0",
    "mocha": "^2.1.0",
    "node-localstorage": "^0.3.6"
  }
}
